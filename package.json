{
  "name": "advent-of-code",
  "private": true,
  "scripts": {
    "codegen": "turbo generate",
    "clean": "turbo clean && git clean -xdf node_modules",
    "format": "turbo format --continue -- --cache --cache-location .cache/.prettiercache",
    "lint": "turbo lint --continue -- --cache --cache-location .cache/.eslintcache",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "dev": "turbo dev --"
  },
  "devDependencies": {
    "@aoc/prettier": "workspace:*",
    "@turbo/gen": "catalog:",
    "prettier": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "prettier": "@aoc/prettier",
  "packageManager": "pnpm@9.14.4",
  "engines": {
    "node": ">=20"
  }
}
